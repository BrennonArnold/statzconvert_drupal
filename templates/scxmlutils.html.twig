{% autoescape false %}
    {% if useNav %}
        <nav class="navbar navbar-inverse">
            <div class="container-fluid">
                {{ navBar }}
            </div>
        </nav>
    {% endif %}
    <div class="panel panel-default">
        <div class="panel-heading">
            <h4 style="font-weight: bold;">{{ pageTitle }}</h4>
        </div>
        <div class="panel-body">
            <div class="checkbox">
                <label>{{ form.chkempty4v }}</label>
            </div>
            <div class="form-group row">
                <div class="col-xs-6" style="border-right: 1px solid #ccc;">
                    {{ form.dbvteam }}
                </div>
                <div class="col-xs-6">
                    {{ form.dbhteam }}
                </div>
            </div>
            <div class="form-group row">
                <div class="col-xs-6" style="border-right: 1px solid #ccc;">
                    {{ form.vteamid }}
                </div>
                <div class="col-xs-6">
                    {{ form.hteamid }}
                </div>
            </div>
            <div class="form-group row">
                <div class="col-xs-6" style="border-right: 1px solid #ccc;">
                    {{ form.filvteamros }}
                </div>
                <div class="col-xs-6">
                    {{ form.filhteamros }}
                </div>
            </div>
            <div class="form-group row">
                <div class="col-xs-6" style="border-right: 1px solid #ccc;">
                    {{ form.filvteamxml }}
                </div>
                <div class="col-xs-6">
                    {{ form.filhteamxml }}
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-xs-12">{{ form.submit }}</div>
    </div>
    {{ form.gameid }}
    {{ form.form_build_id }}
    {{ form.form_token }}
    {{ form.form_id }}
{% endautoescape %}
